@report{hamelinRapportStageObservatoire2019,
  langid = {french},
  location = {{Paris}},
  title = {Rapport de stage – Observatoire de Paris – Projet DISHAS},
  abstract = {A l’issue de mon cursus en DUT Informatique à l’IUT de Montreuil - Université Paris 8, j’ai eu la chance réaliser un stage d’une durée de quatre mois au sein de l’Observatoire de Paris et plus particulièrement de l’équipe de développement du projet DISHAS. Ce rapport a pour but de présenter et détailler toutes les tâches qui m’ont été confiées et les impressions qu’elles m’ont laissées.
Grâce à ce stage, j’ai pu m’ouvrir au domaine des humanités numériques, et aux différentes implications que cela pouvait avoir. Les objectifs de ce stage étaient réaliser une API web RESTful permettant les actions du CRUD, la mise en place d’un moteur de recherche ElasticSearch et l’interfaçage d’un outil de visualisation Kibana sur une plateforme déjà établie.
Le fait de m’ouvrir à un domaine qui, de par son côté métier, m’était totalement inconnu, fut une expérience extrêmement enrichissante que ce soit par son contenu ou par sa valeur quant à l’amélioration de ma capacité d’adaptation face à des besoins totalement nouveaux par rapport à mes expériences et projets précedents.
Au cours de ce rapport, je vais présenter succintement le projet ALFA, dans lequel s’inscrit DISHAS, puis entrer dans les détails de ce dernier, avant de développer les différentes missions et objectifs que j’ai eu durant ce stage. Enfin, je reviendrais sur mes impressions au terme de ce stage.},
  date = {2019-08},
  pages = {33},
  author = {Hamelin, Malcolm},
  keywords = {technique}
}

